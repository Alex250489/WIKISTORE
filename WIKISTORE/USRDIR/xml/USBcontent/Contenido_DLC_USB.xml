<XMBML version="1.0">
<!-- USB WIKISTORE -->
<View id="Contenido_DLC_USB_items"> 
		<Attributes>
            <Table key="descarga">
                <Pair key="icon"><String>/dev_hdd0/game/WIKISTORE/USRDIR/IMAGES/XMB/dlc usb master.png</String></Pair>
                <Pair key="title"><String>Descargar All DLCs PSN al USB</String></Pair>
                <Pair key="info"><String>Se descargaran todos los DLCs existentes de PSN (EU-US-HK-JP) al USB</String></Pair>
                <Pair key="ingame"><String>disable</String></Pair>
            </Table>		
			<Table key="store_01">
                <Pair key="icon"><String>/dev_hdd0/game/WIKISTORE/USRDIR/IMAGES/XMB/dlc usb master.png</String></Pair>
				<Pair key="icon_notation"><String>WNT_XmbItemAlbum</String></Pair>
                <Pair key="title"><String>Todos Los DLCs de PSN (Primero descargar)</String></Pair>
                <Pair key="info"><String>Toda la base de datos de DLCs de PSN en un mismo sitio</String></Pair>
                <Pair key="ingame"><String>Disable</String></Pair>
            </Table>			
		</Attributes>
			<Items>
			<Item class="type:x-xmb/module-action" key="instru" attr="instru"/>
			<Query
                class="type:x-xmb/folder-pixmap"
                key="descarga"
                attr="descarga"
                src="#descarga_link"
            />
			<Query
				 class="type:x-xmb/folder-pixmap"
				 key="store_01"
				 attr="store_01"
				 src="xmb://localhost/dev_usb000/game/Freeshop/USRDIR/han_toolbox_xmb.xml#han_toolbox_xmb_main"
				/>
		</Items>
	</View>
	
	<View id="descarga_link">
        <Attributes>
            <Table key="descarga">
                <Pair key="info"><String>net_package_install</String></Pair>
                <Pair key="pkg_src"><String>https://www.dropbox.com/s/wt8izh92xzzv5sk/All_DLCs_PSN_USB_WIKISTORE.pkg?dl=1</String></Pair>
                <Pair key="pkg_src_qa"><String>https://www.dropbox.com/s/wt8izh92xzzv5sk/All_DLCs_PSN_USB_WIKISTORE.pkg?dl=1</String></Pair>
                <Pair key="content_name"><String>tool_pkg_install_pc</String></Pair>
                <Pair key="content_id"><String>CUSTOM-INSTALLER_00-0000000000000000</String></Pair>
                <Pair key="prod_pict_path"><String>/dev_hdd0/game/WIKISTORE/USRDIR/IMAGES/XMB/download.png</String></Pair>
            </Table>
        </Attributes>
        <Items>
            <Item class="type:x-xmb/xmlnpsignup" key="descarga" attr="descarga"/>
        </Items>
    </View>
		  			
</XMBML>
